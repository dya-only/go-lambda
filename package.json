{
  "name": "lambda",
  "version": "1.0.0",
  "main": "dist/app.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "hello": "NODE_ENV=dev nodemon --watch lambda/ --delay 500ms --exec ts-node lambda/hello.ts",
    "start:dev": "NODE_ENV=dev ts-node-dev --respawn --transpile-only lambda/hello.ts",
    "start": "NODE_ENV=production node build/start.js"
  },
  "dependencies": {
    "@types/aws-lambda": "^8.10.124",
    "aws-lambda": "^1.0.7",
    "esbuild": "^0.19.4",
    "typescript": "^5.2.2"
  }
}
